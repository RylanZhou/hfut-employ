.header {
  position: fixed;
  z-index: 999;
  top: 0;
  box-sizing: border-box;
  width: 750rpx;
  height: 180rpx;
  padding-top: 90rpx;
  padding-bottom: 70rpx;
  text-align: center;
  font-size: 48rpx;
  color: white;
}

.header .background {
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
  width: 100%;
  height: 100%;
}

.header image {
  position: absolute;
  left: 20rpx;
  bottom: 20rpx;
  height: 70rpx;
  width: 101rpx;
}

.header van-icon {
  position: absolute;
  left: 20rpx;
  bottom: 32rpx;
}

.header .title {
  font-size: 40rpx;
}
