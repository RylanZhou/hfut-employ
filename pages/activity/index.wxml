	</view>
	<view class="wx-block">
		<view class="wx-cells">
			<view class="wx-cell-line"  bindtap="clickFormView" data-route="jobfair/index" data-title="网络招聘会">
				<image class="wx-cell-icon" src="/assets/icons/Discovery/market.png"></image>
				<text class="wx-cell-text">网络招聘会</text>
				<image class="wx-cell-arrow" src="/assets/right-gray.png"></image>
			</view>
			<view class="wx-cell"  bindtap="clickFormView" data-route="video/index" data-title="空中宣讲会">
				<image class="wx-cell-icon" src="/assets/icons/Discovery/video.jpg"></image>
				<text class="wx-cell-text">空中宣讲会</text>
				<image class="wx-cell-arrow" src="/assets/right-gray.png"></image>
			</view>
		</view>
	</view>
	<view class="wx-block">
		<view class="wx-cells">
			<view class="wx-cell-line"  bindtap="tempclose" data-title="招聘日历" data-route="recruitment/calendar">
				<image class="wx-cell-icon" src="/assets/icons/Discovery/date.png"></image>
				<text class="wx-cell-text">招聘日历</text>
			</view>
			<view class="wx-cell"  bindtap="tempclose" data-title="专场招聘会" data-route="recruitment/whole">
				<image class="wx-cell-icon" src="/assets/icons/Discovery/recruit.jpg"></image>
				<text class="wx-cell-text">专场招聘会</text>
			</view>
		</view>
	</view>
