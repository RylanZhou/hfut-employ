			<view class="wx-cell-line"  bindtap="clickFormView" data-route="jobinfo/index?kind=1">
				<image class="wx-cell-icon" src="/assets/icons/Mine/experience.png"></image>
				<text class="wx-cell-text">职位信息</text>
				<image class="wx-cell-arrow" src="/assets/right-gray.png"></image>
			</view>
			<view class="wx-cell"  bindtap="clickFormView" data-route="jobinfo/index?kind=2">
